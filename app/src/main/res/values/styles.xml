<resources>
    <style name="Theme.FileExplorer" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/MyActionBarStyle</item>
        <item name="android:windowBackground">@color/tabBgColor</item>
    </style>
    <style name="MyActionBarStyle" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:titleTextStyle">@style/MyActionBarTitleTextStyle</item>
    </style>
    <style name="MyActionBarTitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="MyTabStyle" parent="@android:style/Widget.Holo.Light.ActionBar.TabText">
        <item name="android:background">@*android:drawable/tab_indicator_ab_holo</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">16px</item>
        <item name="android:textColor">#5e5e5e</item>
        <item name="android:layout_width">@dimen/tab_button_width</item>
        <item name="android:layout_height">@dimen/tab_button_height</item>
    </style>
</resources>